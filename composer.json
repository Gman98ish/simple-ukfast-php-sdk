{
    "name": "gman98ish/ukfast-simple-php-sdk",
    "description": "A simpler version of the UKFast PHP SDK.",
    "keywords": [
        "ukfast"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Kelly",
            "email": "george.kelly@ukfast.co.uk",
            "role": "Author & Maintainer"
        }
    ],
    "autoload": {
        "psr-4": {
            "UKFast\\SimpleSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "guzzlehttp/guzzle": ">=5.6.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0",
        "mockery/mockery": "^1.0"
    }
}
